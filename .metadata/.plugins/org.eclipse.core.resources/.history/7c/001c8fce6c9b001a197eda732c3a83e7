	public static void main(String[] args) {
		Scanner obj=new Scanner(System.in);
		System.out.println("Owner of Chennai Super King is :-");
		String s=obj.nextLine();
		System.out.println("Owner of Chennai Super King is :-" +s);
		
		
		
		
		

	}

}
